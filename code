#include<stdio.h>


struct Student
{
	int std_id;
	int age;
	float marks;
};


int main()
{
	struct Student s1;
	printf("Enter the Student ID:    ");
	scanf("%d", &s1.std_id);
	printf("\nEnter the age:    ");
	scanf("%d", &s1.age);
	printf("\nEnter the marks:    ");
	scanf("%f", &s1.marks);
	int k = (s1.marks<0 || s1.marks>100)?1:0;
	int p = (s1.age>20)?0:1;
	if(p == 1 && k == 1)
		printf("Age and Marks are INVALID");
	else if (p==1)
		printf("Age is INVALID");
	else if (k==1)
		printf("Marks are INVALID");
	if (p || k || s1.marks<=65)
		printf("\nNot Eligible for admission");
	else
		printf("\nEligible for admission");
}
